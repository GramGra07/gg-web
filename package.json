{
  "name": "gg-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npx live-server --port=5173",
    "clean": "node -e \"try{require('fs').rmSync('node_modules',{recursive:true,force:true});console.log('Removed node_modules');}catch(e){console.error(e);process.exit(1)}\"",

    
    "sitemap": "node scripts/generate-terminal-sitemap.mjs",
    "predev": "npm run sitemap",
    "dev": "serve public -l 5173",
	"prestart": "npm run sitemap",
    "prebuild": "npm run sitemap",
    "build": "echo \"static site\""
  },
  "devDependencies": {
    "live-server": "^1.2.0",
    "node-fetch": "^3.3.2",
    "rimraf": "^5.0.5",
    "serve": "^14.2.2"
  }
}

